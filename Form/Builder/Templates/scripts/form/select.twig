{% set select = this.getObject() %}
<div class="form-group">
    <label class="sr-only" for="">{{ select.getLabel() }}</label>
    <select {% if select.isMultiple() %}multiple{% endif %} class="form-control">
        {% for key, item in select.getOptions() %}
            <option {% if select.getValue() == key %}selected{% endif %} value="{{ key }}">{{ item }}</option>
        {% endfor %}
    </select>
</div>
