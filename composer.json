{
    "name": "backbee",
    "description": "BackBuilder",
    "type": "backbuilder-package",
    "extra": {
        "installer-name": "BackBuilder"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "http://gitlab.lp-digital.fr/backbuilder/backbuilderapiclient.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "2.2.11",
        "doctrine/orm": "2.5.*@dev",
        "doctrine/dbal": "2.5.*@dev",
        "twig/twig": "1.16.*@dev",
        "twig/extensions" : "1.0.1",
        "swiftmailer/swiftmailer": "5.1.0",
        "jms/serializer": "0.15.0",
        "jms/metadata": "1.5.0",
        "backbuilder/api-client": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "BackBuilder\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phpunit/dbunit": "1.3.1",
        "mikey179/vfsStream": "1.2.0",
        "fzaninotto/faker": "1.3.0"
    },
    "minimum-stability": "dev"
}
