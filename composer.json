{
    "name": "backbee",
    "description": "BackBuilder",
    "type": "backbee-package",
    "extra": {
        "installer-name": "BackBuilder"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "http://gitlab.lp-digital.fr/backbuilder/backbuilderapiclient.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/eric-chau/installers.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "2.3.18",
        "doctrine/orm": "2.4.4",
        "doctrine/dbal": "2.4.2",
        "twig/twig": "1.16.0",
        "twig/extensions" : "1.1.0",
        "swiftmailer/swiftmailer": "5.2.1",
        "jms/serializer": "0.16.0",
        "jms/metadata": "1.5.1",
        "backbuilder/api-client": "dev-master",
        "eric-chau/installers": "dev-master",
        "willdurand/jsonp-callback-validator": "~1.0",
        "symfony/expression-language": "2.5.4"
    },
    "autoload": {
        "psr-4": {
            "BackBuilder\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.4",
        "phpunit/dbunit": "1.3.1",
        "mikey179/vfsStream": "1.3.0",
        "fzaninotto/faker": "1.4.0"
    },
    "minimum-stability": "dev"
}
